  "license": "GPL-3.0-or-later",
  "private": false,
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-console": "^1.1.0",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@storybook/storybook-deployer": "^2.8.1",
    "babel-loader": "^8.0.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "husky": "^3.0.3",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "webpack": "^4.39.2"
  },
  "main": "dist/index.js",
  "scripts": {
    "preversion": "yarn lint && yarn build",
    "postversion": "git push --tags && yarn publish --access public --new-version $npm_package_version . && git push && echo \"Successfully released version $npm_package_version!\"",
    "postpublish": "yarn deploy",
    "lint": "yarn eslint --fix --ignore-path './.gitignore' --ext '.js,.json,.md' './'",
    "lint-staged": "yarn eslint --fix",
    "start": "start-storybook",
    "build": "yarn build:lib",
    "build:lib": "babel src --out-dir dist --copy-files",
    "deploy": "yarn deploy:storybook",
    "deploy:storybook": "storybook-to-ghpages"
  },
  "dependencies": {
    "@material-ui/core": "^4.3.2",
    "@material-ui/styles": "^4.3.0",
    "dequal": "^1.0.0",
    "geojson": "^0.5.0",
    "leaflet": "^1.5.1",
    "lodash": "^4.17.14",
    "prop-types": "^15.7.2",
    "react-content-loader": "^4.2.2",
    "react-share": "^3.0.1",
    "shortid": "^2.2.15",
    "victory": "^32.3.1"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.3.2",
    "@material-ui/styles": "^4.3.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,json,md}": [
      "yarn lint-staged",
      "git add"
    ]
  },
  "engines": {
    "node": ">=10.16.0 <=12.x"
  }
