{
  "name": "@codeforafrica/hurumap-ui",
  "version": "0.2.2",
  "description": "HURUmap charts, map and other components.",
  "repository": "https://github.com/CodeForAfrica/HURUmap-UI.git",
  "author": "Code for Africa <hello@codeforafrica.org> (https://codeforafrica.org)",
  "license": "GPL-3.0-or-later",
  "private": false,
  "devDependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-console": "^1.1.0",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@storybook/storybook-deployer": "^2.8.1",
    "apollo-boost": "^0.4.7",
    "babel-loader": "^8.0.5",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.7.0",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "husky": "^3.0.3",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "webpack": "^4.39.2"
  },
  "main": "core/index.js",
  "scripts": {
    "preversion": "yarn lint && yarn build",
    "postversion": "yarn publish --access public --new-version $npm_package_version . && git push --tags && (git diff-index --quiet HEAD || git push) && echo \"Successfully released version $npm_package_version!\"",
    "postpublish": "yarn deploy",
    "lint": "yarn eslint --fix --ignore-path './.gitignore' --ext '.js,.json,.md' './'",
    "lint-staged": "yarn eslint --fix --ignore-path './.gitignore' --ext '.js,.json,.md'",
    "start": "start-storybook -s ./",
    "build": "yarn build:lib",
    "build:lib": "babel src/core --out-dir core --copy-files && babel src/factory --out-dir factory --copy-files && babel src/components --out-dir components --copy-files && babel src/cms --out-dir cms --copy-files",
    "deploy": "yarn deploy:storybook",
    "deploy:storybook": "storybook-to-ghpages",
    "publish:local": "yarn build && mkdir -p ../publish-local && yarn pack --filename ../publish-local/codeforafrica-hurumap-ui-$npm_package_version.tar && cd ../publish-local && tar xopf codeforafrica-hurumap-ui-$npm_package_version.tar && cd package && yarn link"
  },
  "dependencies": {
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "@types/react": "^16.9.11",
    "apollo-client": "^2.6.8",
    "apollo-utilities": "^1.3.3",
    "dequal": "^1.0.0",
    "dom-to-image": "^2.6.0",
    "geojson": "^0.5.0",
    "leaflet": "^1.5.1",
    "lodash": "^4.17.14",
    "prop-types": "^15.7.2",
    "react-content-loader": "^4.2.2",
    "react-share": "^4.0.0",
    "shortid": "^2.2.15",
    "victory": "^33.1.1"
  },
  "peerDependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "apollo-client": "^2.6.8",
    "apollo-utilities": "^1.3.3",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,json,md}": [
      "yarn lint-staged",
      "git add"
    ]
  },
  "engines": {
    "node": ">=10.16.0 <=12.x"
  }
}
